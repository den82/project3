{% extends 'base.html.twig'  %}

{% block title %} Question: {{ question }} {% endblock %}

{% block body %}
<h1>{{ question }}</h1>

{# Comment #}

<div>
    Question number 1
</div>

<h2>Answers ({{ answers|length }})</h2>

<ul>
{% for answer in answers %}
    <li>{{ answer }}</li>
{% endfor %}
</ul>

{% endblock %}